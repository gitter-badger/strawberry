{
  "name": "strawberry",
  "version": "1.0.0-alpha.2",
  "description": "The Sass Framework",
  "keywords": [
    "sass",
    "framework",
    "component",
    "theme",
    "css"
  ],
  "homepage": "https://github.com/strawberrysass/strawberry/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/strawberrysass/strawberry/issues"
  },
  "license": "MIT",
  "contributors": [
    "Maxime GÃ©linas <maxime.gelinas@idmobiles.com>"
  ],
  "main": "_strawberry.scss",
  "repository": {
    "type": "git",
    "url": "https://github.com/strawberrysass/strawberry.git"
  },
  "scripts": {
    "test": "mocha",
    "build": "node-sass ./ -o ./ --include-path ./src",
    "build:w": "node-sass ./ -o ./ --include-path ./src -w",
    "demo": "cd ./demo && lite-server",
    "doc": "sassdoc ./src -c ./.sassdoc.yml",
    "postdoc": "cd ./doc && lite-server",
    "lint": "sass-lint ./**/*.scss -c ./.sass-lint.yml -v -i ./node_modules/**"
  },
  "dependencies": {},
  "devDependencies": {
    "lite-server": "^2.2.2",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "sass-lint": "^1.8.2",
    "sass-true": "^2.1.3",
    "sassdoc": "^2.1.20"
  }
}
